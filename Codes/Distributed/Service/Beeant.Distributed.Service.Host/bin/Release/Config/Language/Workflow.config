<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <Dislan>
    <XmlLanguage>
      <!--条件类型-->
      <Language Name="Beeant.Domain.Entities.Workflow.ConditionType">
        <Message Name="Create" Value="满足条件生成"></Message>
        <Message Name="UnCreate" Value="满足条件不生成"></Message>
        <Message Name="Skip" Value="满足条件跳过"></Message>
